{
  "extends": "tslint:recommended",
  "defaultSeverity": "warning",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": true,
    "no-console": {
      "severity": "warning",
      "options": [
        "info",
        "log"
      ]
    }
  }
}
